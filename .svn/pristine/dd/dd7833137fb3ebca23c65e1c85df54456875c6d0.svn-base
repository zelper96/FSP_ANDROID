        <!-- 301호   -->
        <android.support.constraint.ConstraintLayout
            android:id="@+id/Layout_301"
            android:layout_width="wrap_content"
            android:layout_height="65dp"
            >

            <TextView
                android:id="@+id/name_301"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:textSize="30dp"
                android:textColor="#000"
                android:fontFamily="@font/nanummyeongjobold"
                android:text="박영애"
                />

            <TextView
                android:id="@+id/age_301"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toRightOf="@id/name_301"
                app:layout_constraintTop_toTopOf="@id/name_301"
                android:layout_marginLeft="3dp"
                android:textSize="30dp"
                android:textColor="#000"
                android:fontFamily="@font/nanummyeongjobold"
                android:text="(90세)"
                />


            <TextView
                android:id="@+id/text_list_301"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/name_301"
                app:layout_constraintLeft_toLeftOf="@id/name_301"
                android:layout_marginTop="2.5dp"
                android:textSize="10dp"
                android:textColor="#29166F"
                android:fontFamily="@font/nanummyeongjobold"
                android:maxLines="2"
                android:text="김미선\n김미선"

                />

        </android.support.constraint.ConstraintLayout>


