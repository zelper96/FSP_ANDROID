<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="4dp"
    >





        <com.nexgrid_fsp.myapplication.thumbil.Style.Thumbnail_sub_Split_V
            android:id="@+id/view_page_sub_0"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@id/view_center"
            app:layout_constraintTop_toTopOf="parent"
            >
        </com.nexgrid_fsp.myapplication.thumbil.Style.Thumbnail_sub_Split_V>


    <View
        android:id="@+id/view_center"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />


        <com.nexgrid_fsp.myapplication.thumbil.Style.Thumbnail_sub_Split_V
            android:id="@+id/view_page_sub_1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/view_center"
            app:layout_constraintBottom_toBottomOf="parent"
            >
        </com.nexgrid_fsp.myapplication.thumbil.Style.Thumbnail_sub_Split_V>





</android.support.constraint.ConstraintLayout>